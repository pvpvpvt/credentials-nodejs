    "@types/expect.js": "^0.3.29",
    "@types/ini": "^1.3.30",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.10",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "expect.js": "^0.3.1",
    "mm": "^2.4.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
